&lt;p style=&quot; margin-top:12px; margin-bottom:12px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;img src=&quot;file:///C:/Users/Habtamu/AppData/Local/Packages/oice_16_974fa576_32c1d314_2c2d/AC/Temp/msohtmlclip1/01/clip_image008.png&quot; width=&quot;57&quot; height=&quot;46&quot; /&gt;&lt;span style=&quot; font-size:18pt;&quot;&gt;  &lt;/span&gt;&lt;img src=&quot;file:///C:/Users/Habtamu/AppData/Local/Packages/oice_16_974fa576_32c1d314_2c2d/AC/Temp/msohtmlclip1/01/clip_image010.png&quot; width=&quot;65&quot; height=&quot;43&quot; /&gt;&lt;span style=&quot; font-size:18pt;&quot;&gt; = 0.25msec&lt;/span&gt;&lt;span style=&quot; font-size:8pt;&quot;&gt; &lt;/span&gt;&lt;/p&gt;
&lt;p style=&quot; margin-top:12px; margin-bottom:12px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;span style=&quot; font-size:18pt;&quot;&gt; &lt;/span&gt;&lt;span style=&quot; font-size:8pt;&quot;&gt; &lt;/span&gt;&lt;/p&gt;
